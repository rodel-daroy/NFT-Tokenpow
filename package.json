  "name": "tokenpow",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadless",
    "test-coverage": "ng test --no-watch --code-coverage",
    "build:prod": "ng build --prod",
    "build:dev": "ng build --configuration development",
    "build:maintenance": "ng build --configuration maintenance",
    "lint": "ng lint",
    "e2e": "cypress open",
    "e2e-test": "./node_modules/.bin/cypress run",
    "deploy:firebase": "firebase use token-pow && ng build --prod && npm run post-build && firebase deploy --only hosting:token-pow",
    "deploy:firebase-dev": "firebase use tokenpowdev && ng build --configuration development && firebase deploy --only hosting:tokenpowdev",
    "deploy:firebase:maintenance-mode:prod": "firebase use token-pow  && ng build --configuration maintenance && firebase deploy --only hosting:token-pow",
    "deploy:firebase:maintenance-mode:dev": "firebase use tokenpowdev && ng build --configuration maintenance && firebase deploy --only hosting:tokenpowdev",
    "deploy:dev": "firebase deploy --only hosting:tokenpowdev",
    "deploy:prod": "firebase deploy --only hosting:token-pow",
    "post-build": "node ./build/post-build.js",
    "postinstall": "node version.js",
    "i18n:init": "ngx-translate-extract --input ./src --output ./src/assets/i18n/template.json --key-as-default-value --replace --format json",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/{en,es,sk,ch,ro}.json --key-as-default-value --clean --sort --format json",
    "deploy:preview": "firebase hosting:channel:deploy --expires 2d previewGoBitFundMeApp",
    "emulators:start": "firebase emulators:start",
    "use-dev": "firebase use tokenpowdev",
    "lite": "lite-server",
    "use-prod": "firebase use token-pow"
  },
